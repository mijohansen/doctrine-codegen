{
  "name": "mijohansen/doctrine-codegen",
  "description": "Small lib to share common functions working with Doctrine.",
  "type": "library",
  "version": "0.0.2",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Michael Johansen",
      "email": "michael.johansen@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "doctrine/dbal": "2.5.13",
    "doctrine/inflector": "1.1.0",
    "doctrine/instantiator": "1.0.5",
    "gossi/php-code-generator": "^0.5.0",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "DbUtil\\": "src"
    }
  }
}
